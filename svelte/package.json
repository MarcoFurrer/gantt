{
	"name": "wx-svelte-gantt",
	"version": "2.0.0-beta-2",
	"description": "Interactive and customizable Gantt chart component written in Svelte",
	"productTag": "gantt",
	"needsTrial": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --mode test --host 0.0.0.0  --port 5100",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/svar-widgets/gantt.git"
	},
	"bugs": {
		"url": "https://forum.svar.dev"
	},
	"homepage": "https://svar.dev/svelte/gantt/",
	"dependencies": {
		"wx-gantt-data-provider": "2.0.0-beta-2",
		"wx-gantt-locales": "2.0.0-beta-2",
		"wx-gantt-store": "2.0.0-beta-2",
		"wx-lib-dom": "0.6.0",
		"wx-lib-state": "1.9.0",
		"wx-lib-svelte": "0.4.0",
		"wx-svelte-core": "2.0.0-beta-2",
		"wx-svelte-menu": "2.0.0-beta-2",
		"wx-svelte-grid": "2.0.0-beta-2",
		"wx-svelte-toolbar": "2.0.0-beta-2"
	},
	"files": [
		"src",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	],
	"keywords": [
		"svelte",
		"gantt",
		"gantt-chart",
		"svelte-gantt",
		"svelte-gantt-chart",
		"gantt chart",
		"gantt-chart-component",
		"gantt-diagram",
		"svelte-gantt-component",
		"svar widgets",
		"svar gantt",
		"timeline"
	]
}
